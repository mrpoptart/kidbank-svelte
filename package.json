{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear --port 8080",
    "emulate": "firebase emulators:start",
    "deploy": "firebase deploy",
    "cypress:test": "cypress run",
    "cypress:open": "cypress open",
    "rules:test": "mocha",
    "rules:push": "firebase deploy --only firestore:rules",
    "sync": "npm run build && npx cap sync",
    "android:open": "npx cap open android",
    "android:sha1": "echo \"Look in the google cloud secret manager\" && echo \"What's the store pass?\" && read storepass && echo \"What's the keypass?\" && read keypass && keytool -list -v -keystore ./kidbank.keystore -alias kidbank -storepass $storepass -keypass $keypass",
    "android:debug:sha1": "keytool -list -v -keystore ./debug.keystore -alias android -storepass android -keypass android",
    "assets": "npx @capacitor/assets generate --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#1C1C1C' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#1C1C1C'"
  },
  "devDependencies": {
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^5.3.0",
    "@firebase/rules-unit-testing": "^2.0.5",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^5.0.2",
    "cypress": "^12.4.0",
    "firebase-admin": "^11.5.0",
    "mocha": "^10.2.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.44.3",
    "svelte-gestures": "^1.4.1"
  },
  "dependencies": {
    "@capacitor-firebase/authentication": "^5.1.0",
    "@capacitor/android": "^5.3.0",
    "@capacitor/core": "^5.3.0",
    "@capacitor/ios": "^5.3.0",
    "dayjs": "^1.10.7",
    "firebase": "^9.23.0",
    "nanoid": "^3.1.30",
    "sirv-cli": "^1.0.0",
    "sveltestrap": "^5.6.3"
  }
}
